import java.awt.Dimension;
import javax.swing.JApplet;
import javax.swing.JFrame;

public class Cheung_28_12 {
	public static void main(String[] args) {
		  JFrame frame = new JFrame("NineTail");
		  JApplet applet = new CheungNineTail();
		  frame.add(applet); // puts NineTail class into frame
		  frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		  frame.setSize(800, 500); //frame size
		  frame.setMinimumSize(new Dimension(frame.getWidth(), frame.getHeight()));
		  frame.setLocationRelativeTo(null);
		  frame.setVisible(true); //visible frame
		 }
}
